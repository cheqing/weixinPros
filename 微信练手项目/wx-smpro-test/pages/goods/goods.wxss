.slideBar {
  width: 100%;
  height: 100%;
  background-color: #F6F8F8;
  display: flex;
  flex-direction: row;
  border: 1px solid black;
}

.slideBar_left {
  width: 25%;
  height: 100%;
  text-align: center;
}

.slideBar_left_item {
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-bottom: 1px solid #dedede;
}

.slideBar_right {
  width: 75%;
  height: 100%;
}

.slideBar_right image {
  width: 200rpx;
  height: 70rpx;
}

.slideBar_right text {
  font-size: 26rpx;
}

.section {
  margin-bottom: 10rpx;
}